
public class DoublyCircular {
		static class Node{
			private int data;
			private Node next, prev;
			public Node(int value) {
				data = value;
				next = prev = null;
			}
		}

		private Node head;
		private int size;
		
		public DoublyCircular() {
			head = null;
			size = 0;
		}
		
		public boolean isEmpty() {
			return head == null;
		}
		
		public int size() {
			return size;
		}
}
