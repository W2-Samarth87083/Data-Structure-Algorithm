	public static void main(String [] args) {
		int [] arr = {10,20,30,40,50};
		
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter Number to be searched : ");
		int num = sc.nextInt();
		
		int index = linearSearch(arr, num);
		
		if(index == -1)
