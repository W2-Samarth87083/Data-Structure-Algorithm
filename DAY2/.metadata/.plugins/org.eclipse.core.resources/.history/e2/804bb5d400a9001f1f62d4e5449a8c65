

 class Stack2{
	int arr[];
	int top;
	int SIZE;
	public Stack2( int size) {
		SIZE = size;
		this.arr = new int [SIZE];
		this.top = SIZE-1;
	}
	
	public void push(int value) {
		top--;
		arr[top]=value;
	}
	
	public int pop() {
	    int temp=arr[top];
		top++;
		return temp;
	}
	
	public int peek() {
		return arr[top];
	}
	
	public boolean isEmpty() {
		return top == SIZE-1;
	}
	public boolean isFull() {
		return top == -1;
	}

	
	
	
	
}
 
public class Question4 {

	
}
