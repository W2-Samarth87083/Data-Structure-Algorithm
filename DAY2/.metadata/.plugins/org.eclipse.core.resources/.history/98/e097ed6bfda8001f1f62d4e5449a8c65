
class Stack1 {
	int arr[];
	int top;
	int SIZE;

	public Stack1(int size) {
		SIZE = size;
		this.arr = new int[SIZE];
		this.top = -1;
	}

	public void push(int value) {
		top++;
		arr[top] = value;
	}

	public int pop() {
		int temp = arr[top];
		top--;
		return temp;
	}

	public int peek() {
		return arr[top];
	}

	public boolean isEmpty() {
		return top == SIZE - 1;
	}

	public boolean isFull() {
		return top == -1;
	}

}

public class Question3 {

	public static void main(String[] args) {
		Stack1 s = new Stack1(4);
		s.push(10);
		s.push(20);
		s.push(30);
	
		for (int i = 0; i <= s.SIZE; i++) { System.out.print(SIZE[i] + " ");

	}
}
